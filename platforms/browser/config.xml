<?xml version='1.0' encoding='utf-8'?>
<widget id="com.CHH162.SecuredHealthcareApp" version="1.0.0" versionCode="" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>
        SecuredHealthcareApp
    </name>
    <description>
        A secured Healthcare Mobile Application
    </description>
    <author email="jeffreyhung@hotmail.com.tw" href="https://github.com/Jeffreyhung">
        Chi-Heng Hung
    </author>
    <content src="index.html" />
    <icon platform="android" qualifier="ldpi" src="res/icon/android/ldpi.png" />
    <icon platform="android" qualifier="mdpi" src="res/icon/android/mdpi.png" />
    <icon platform="android" qualifier="hdpi" src="res/icon/android/hdpi.png" />
    <icon platform="android" qualifier="xhdpi" src="res/icon/android/xhdpi.png" />
    <icon platform="android" qualifier="xxhdpi" src="res/icon/android/xxhdpi.png" />
    <icon platform="android" qualifier="xxxhdpi" src="res/icon/android/xxxhdpi.png" />
    <icon src="icon.png" />
    <access launch-external="yes" origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>
