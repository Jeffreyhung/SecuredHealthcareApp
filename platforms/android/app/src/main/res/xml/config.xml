<?xml version='1.0' encoding='utf-8'?>
<widget id="com.CHH162.SecuredHealthcareApp" version="1.1.0" versionCode="10" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="EmailComposer">
        <param name="android-package" value="de.appplant.cordova.emailcomposer.EmailComposer" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <name>
        Secured Healthcare App
    </name>
    <description>
        A secured Healthcare Mobile Application
    </description>
    <author email="jeffreyhung@hotmail.com.tw" href="https://github.com/Jeffreyhung">
        Chi-Heng Hung
    </author>
    <content src="index.html" />
    <icon src="www/img/icon.png" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="market:*" />
    <icon qualifier="ldpi" src="www/img/icons/android/drawable-ldpi-icon.png" />
    <icon qualifier="mdpi" src="www/img/icons/android/drawable-mdpi-icon.png" />
    <icon qualifier="hdpi" src="www/img/icons/android/drawable-hdpi-icon.png" />
    <icon qualifier="xhdpi" src="www/img/icons/android/drawable-xhdpi-icon.png" />
    <icon qualifier="xxhdpi" src="www/img/icons/android/drawable-xxhdpi-icon.png" />
    <icon qualifier="xxxhdpi" src="www/img/icons/android/drawable-xxxhdpi-icon.png" />
    <splash qualifier="land-ldpi" src="www/img/screens/android/drawable-land-ldpi-screen.png" />
    <splash qualifier="land-mdpi" src="www/img/screens/android/drawable-land-mdpi-screen.png" />
    <splash qualifier="land-hdpi" src="www/img/screens/android/drawable-land-hdpi-screen.png" />
    <splash qualifier="land-xhdpi" src="www/img/screens/android/drawable-land-xhdpi-screen.png" />
    <splash qualifier="land-xxhdpi" src="www/img/screens/android/drawable-land-xxhdpi-screen.png" />
    <splash qualifier="land-xxxhdpi" src="www/img/screens/android/drawable-land-xxxhdpi-screen.png" />
    <splash qualifier="port-ldpi" src="www/img/screens/android/drawable-port-ldpi-screen.png" />
    <splash qualifier="port-mdpi" src="www/img/screens/android/drawable-port-mdpi-screen.png" />
    <splash qualifier="port-hdpi" src="www/img/screens/android/drawable-port-hdpi-screen.png" />
    <splash qualifier="port-xhdpi" src="www/img/screens/android/drawable-port-xhdpi-screen.png" />
    <splash qualifier="port-xxhdpi" src="www/img/screens/android/drawable-port-xxhdpi-screen.png" />
    <splash qualifier="port-xxxhdpi" src="www/img/screens/android/drawable-port-xxxhdpi-screen.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="phonegap-version" value="cli-8.1.1" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="AndroidExtraFilesystems" value="files,cache, sdcard, cache-external, files-external" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="true" />
</widget>
